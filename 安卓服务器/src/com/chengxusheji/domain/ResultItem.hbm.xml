<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="ResultItem" table="ResultItem">

          <!-- 主键记录编号 -->
          <id name="resultItemId" column="resultItemId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 结果描述 -->
          <property
            name="resultItemText"
            column="resultItemText"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
